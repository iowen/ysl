@{
    Layout = null;
        var urlPrefix = "http://guerilladevteam.com";
 
}
<link rel="stylesheet" href="~/Content/SiteTheme/style.css">
<link rel="stylesheet" href="~/Content/SiteTheme/colors/default.css" id="color">

<div class="home">
