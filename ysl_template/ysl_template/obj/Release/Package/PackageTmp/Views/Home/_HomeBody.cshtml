    <!-- #main-content -->



    <footer id="footer">
        <div class="wrap group">

            <!-- widget-block -->

            <div class="site-copy">
                <div style="float:left;"><a href="@Request.Url.GetLeftPart(UriPartial.Authority)/Administrator">Admin</a></div>
                <span>&copy; @DateTime.Now.Year Kontractz Ent.  All rights reserved.</span>
