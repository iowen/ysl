                                        <time class="dtstart" datetime="@e.Start.ToString("yyyy-MM-dd HH:mm")" title="@e.Start.ToString("yyyy-MM-dd HH:mm")">
                                            @e.Start.ToString("MMMM dd, yyyy h:mm tt")
