                                    @*<a href="#" class="ci-read-more">Read More</a>*@
                                </div>
                                <!-- sld-desc -->
                            </li>
                        </ul>
                    </div>
                    <!-- .flexslider -->
                </div>
                <!-- .sld-container -->

                <section class="widget-block group">
                    <div class="block one-half ">

                        <h3>Latest Release</h3>
                        <a href="@Request.Url.GetLeftPart(UriPartial.Authority)/Music/Listen?aid=@ViewBag.latestSingle.audioAlbumId"><img src="@urlPrefix@ViewBag.latestSingle.photo.Location" alt="" width="380" height="220"></a>
                        <h4><a href="@Request.Url.GetLeftPart(UriPartial.Authority)/Music/Listen?aid=@ViewBag.latestSingle.audioAlbumId"> @ViewBag.latestSingle.title</a></h4>
                        <p>@ViewBag.latestSingle.title</p>

                    </div>
                    <div class="block one-half ">
                        <h3>New Photos</h3>
                        <a href="@Request.Url.GetLeftPart(UriPartial.Authority)/Photo/ViewAlbum?aid=@ViewBag.uploads.photoAlbums[0].album.PhotoAlbumId"><img src="@urlPrefix@ViewBag.uploads.photoAlbums[0].items[0].Photo.Location" alt="" width="380" height="220"></a>
                        <h4><a href="@Request.Url.GetLeftPart(UriPartial.Authority)/Photo/ViewAlbum?aid=@ViewBag.uploads.photoAlbums[0].album.PhotoAlbumId">@ViewBag.uploads.photoAlbums[0].album.Title</a></h4>
                        <p>@ViewBag.uploads.photoAlbums[0].album.Description</p>
                    </div>
